{
    "parser": "babel-eslint",
    "plugins": [ "react" ],
    "rules": {
      "eol-last": ["error", "always"],
      "no-multi-spaces": "error", 
      "no-console": ["warn", { "allow": ["info", "error"] }],
      "no-multiple-empty-lines": [2, {"max": 2, "maxEOF": 1}],
      "react/prop-types": ["off"],
      "indent": ["error", 2],
      "quotes": ["error","single"],
      "semi": ["error","always"]
    }
  }
  